{
  "name": "broccoli-image-min",
  "version": "1.1.0",
  "description": "Broccoli plugin for minifying .jpg, .png, .svg and .gif with imagemin",
  "main": "lib/index.js",
  "author": "Harris Miller",
  "contributors": [
    "Stefan Pfaffel"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stfsy/broccoli-image-min"
  },
  "scripts": {
    "test": "mocha test/specs",
    "test-watch": "mocha test/spec --watch",
    "preversion": "npm run test",
    "version": "npm run changelog && git add -A CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "release-major": "npm version major -m \"chore(release): v%s\"",
    "release-minor": "npm version minor -m \"chore(release): v%s\"",
    "release-patch": "npm version patch -m \"chore(release): v%s\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "keywords": [
    "broccoli-plugin",
    "broccoli",
    "plugin",
    "compress",
    "minify",
    "image",
    "img",
    "gif",
    "jpeg",
    "jpg",
    "png",
    "svg"
  ],
  "dependencies": {
    "broccoli-caching-writer": "^3.0.3",
    "fs-promise": "^2.0.3",
    "globby": "^6.0.0",
    "imagemin": "^5.2.2",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "mocha": "^3.0.2"
  }
}
